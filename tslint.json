{
	"extends": [
		"tslint:recommended"
	],
	"rules": {
		"object-literal-sort-keys": false,
		"member-ordering": false,
		"ordered-imports": false,
		"quotemark": [true, "single"],
		"no-unused-expression": false,
		"indent": [true, "tabs", 4]
	},
	"defaultSeverity": "error",
	"compilerOptions": {
		"sourceMap": true
	},
	"linterOptions": {
		"exlude": [
			"test/*.spec.ts"
		]
	}
}